<package>
  <description brief="libpointmatcher">

     Fetches libpointmatcher through git submodule and makes it available to ROS

  </description>
  <author>Stéphane Magnenat, François Pomerleau</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/libpointmatcher</url>
  <rosdep name="doxygen"/>
  <rosdep name="boost"/>
  <rosdep name="eigen"/>
  <rosdep name="yaml-cpp"/>
  <depend package="libnabo"/>
  <export>
    <cpp cflags="-I${prefix}/include" lflags="-L${prefix}/lib -lpointmatcher" />
    <rosdoc config="rosdoc.yaml" />
  </export>
</package>


